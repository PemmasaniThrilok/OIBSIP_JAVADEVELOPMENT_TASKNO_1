<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0"> <display-name>OnlineReservationSystem</display-name>
  
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>projectServlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>projectServlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>Reservation</servlet-name>
    <servlet-class>projectServlet.ReservationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Reservation</servlet-name>
    <url-pattern>/ReservationServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Cancel</servlet-name>
    <servlet-class>projectServlet.CancellationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Cancel</servlet-name>
    <url-pattern>/CancellationServlet</url-pattern>
  </servlet-mapping>
  
  
</web-app>